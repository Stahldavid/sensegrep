{
  "name": "@sensegrep/core",
  "version": "0.1.11",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && node -e \"const fs = require('fs'); fs.mkdirSync('dist/tool', { recursive: true }); fs.copyFileSync('src/tool/sensegrep.txt', 'dist/tool/sensegrep.txt');\"",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@huggingface/transformers": "^3.0.0",
    "@lancedb/lancedb": "^0.23.0",
    "@parcel/watcher": "^2.4.1",
    "@zip.js/zip.js": "^2.7.0",
    "onnxruntime-node": "^1.18.0",
    "picomatch": "^4.0.2",
    "tree-sitter-wasms": "^0.1.5",
    "web-tree-sitter": "^0.21.0",
    "xdg-basedir": "^5.1.0",
    "zod": "^4.2.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Stahldavid/sensegrep.git"
  },
  "homepage": "https://github.com/Stahldavid/sensegrep",
  "bugs": {
    "url": "https://github.com/Stahldavid/sensegrep/issues"
  }
}
